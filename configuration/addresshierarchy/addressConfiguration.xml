<addressConfiguration> 
    <wipe>true</wipe> 
    <addressComponents> 
        <addressComponent> 
            <field>COUNTRY</field> 
            <nameMapping>ADDRESSHIERARCHY_COUNTRY</nameMapping> 
            <sizeMapping>80</sizeMapping> 
            <elementDefault></elementDefault> 
            <requiredInHierarchy>false</requiredInHierarchy> 
        </addressComponent>  
        <addressComponent> 
            <field>STATE_PROVINCE</field> 
            <nameMapping>ADDRESSHIERARCHY_PROVINCE</nameMapping> 
            <sizeMapping>80</sizeMapping> 
            <requiredInHierarchy>false</requiredInHierarchy> 
        </addressComponent> 
        <addressComponent> 
            <field>COUNTY_DISTRICT</field> 
            <nameMapping>ADDRESSHIERARCHY_DIVISION</nameMapping> 
            <sizeMapping>80</sizeMapping> 
            <requiredInHierarchy>false</requiredInHierarchy> 
        </addressComponent> 
        <addressComponent> 
            <field>ADDRESS_3</field> 
            <nameMapping>ADDRESSHIERARCHY_SUBDIVISION</nameMapping> 
            <sizeMapping>80</sizeMapping> 
            <requiredInHierarchy>false</requiredInHierarchy> 
        </addressComponent> 
        <addressComponent> 
            <field>CITY_VILLAGE</field> 
            <nameMapping>ADDRESSHIERARCHY_CITY</nameMapping> 
            <sizeMapping>80</sizeMapping> 
            <requiredInHierarchy>false</requiredInHierarchy> 
        </addressComponent> 
        <addressComponent> 
            <field>ADDRESS_2</field> 
            <nameMapping>ADDRESSHIERARCHY_DISTRICT</nameMapping> 
            <sizeMapping>80</sizeMapping> 
            <requiredInHierarchy>false</requiredInHierarchy> 
        </addressComponent> 
        <addressComponent> 
            <field>ADDRESS_1</field> 
            <nameMapping>ADDRESSHIERARCHY_AREA</nameMapping> 
            <sizeMapping>80</sizeMapping> 
            <requiredInHierarchy>false</requiredInHierarchy> 
        </addressComponent> 
    </addressComponents> 
    <lineByLineFormat> 
        <string>address1,address2</string> 
        <string>cityVillage,address3</string> 
        <string>countyDistrict, stateProvince</string> 
        <string>country</string>
    </lineByLineFormat> 
    <addressHierarchyFile> 
        <filename>address-hierarchy-entries.csv</filename> 
        <entryDelimiter>\|</entryDelimiter> 
        <identifierDelimiter>^</identifierDelimiter> 
    </addressHierarchyFile> 
</addressConfiguration> 
